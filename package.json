{
  "name": "str-to-regexp",
  "version": "1.1.0",
  "description": "Convert string to regexp.",
  "license": "Apache-2.0",
  "main": "build",
  "scripts": {
    "clean": "rimraf \"build/!(.git)\"",
    "build": "npm run clean && babel src -d build -s",
    "dev": "npm run clean && run-p dev:*",
    "dev:lib": "babel src -d build -sw",
    "dev:tests": "jest --testMatch=\"**/tests/**/*.js\" --watch",
    "start": "npm run dev",
    "test": "jest --testMatch=\"**/tests/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/borela/str-to-regexp.git"
  },
  "bugs": {
    "url": "https://github.com/borela/str-to-regexp/issues"
  },
  "homepage": "https://github.com/borela/str-to-regexp#readme",
  "keywords": [
    "convert",
    "string",
    "regexp"
  ],
  "author": {
    "name": "Alexandre Borela",
    "email": "alexandre@borela.tech",
    "url": "http://www.borela.tech"
  },
  "devDependencies": {
    "babel-preset-ctrine-lib": "^1.0.0",
    "ctrine-dev-preset": "^4.0.0"
  }
}
